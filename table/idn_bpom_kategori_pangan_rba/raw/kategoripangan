    <section id="departments" class="departments" style="margin-top: 130px;">
      <div class="container">
        <div class="section-title">
          <h2 style="text-transform: uppercase;">DAFTAR KATEGORI PANGAN</h2>
        </div>
        <table id="tableKategoriPangan" class="table table-striped table-hover">
          <thead>
            <tr id="filterboxrow">
              <th>Kategori</th>
              <th>Nama</th>
              <th>Keterangan</th>
              <th>Status</th>
            </tr>
            <tr>
              <th>Kategori</th>
              <th>Nama</th>
              <th>Keterangan</th>
              <th>Status</th>
            </tr>          
