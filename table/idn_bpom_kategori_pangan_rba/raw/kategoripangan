  <script>
    $(document).ready(function() {
      var table = $('#tableKategoriPangan').DataTable(
        {
          "language":{
            "url":"https://cdn.datatables.net/plug-ins/1.10.9/i18n/Indonesian.json",
            "sEmptyTable":"Tidak ada data"
          }
        });
      
  
      $('#tableKategoriPangan thead tr#filterboxrow th').each(function() {
  
          var title = $('#tableKategoriPangan thead tr#filterboxrow th').eq($(this).index()).text();
  
          $(this).html('<input id="input' + $(this).index() + '" type="text" class="form-control" placeholder="'+ title + '" />')
              .css('padding-left', '4px');
          $(this).on('keyup change', function() {
              table.column($(this).index()).search($('#input' + $(this).index()).val()).draw();
          });
      });
  
  });
  </script>	</main>
